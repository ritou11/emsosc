{
  "name": "emsosc",
  "version": "0.0.1",
  "description": "A lite oscilloscope for debugging in EMS lab",
  "main": "public/main.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-start": "electron .",
    "electron-pack": "yarn build > /dev/null && build -c.extraMetadata.main=build/main.js"
  },
  "homepage": "./",
  "build": {
    "appId": "com.nogeek.emsosc",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "target": "dir"
    }
  },
  "author": "Nogeek",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^1.5.1",
    "axios": "^0.18.0",
    "electron-is-dev": "^0.3.0",
    "highcharts": "^6.1.1",
    "highcharts-react-official": "^1.4.0",
    "lodash": "^4.17.10",
    "net": "^1.0.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-scripts": "1.1.5"
  },
  "devDependencies": {
    "concurrently": "^4.0.0",
    "electron": "^2.0.8",
    "electron-builder": "^20.28.2",
    "electron-react-devtools": "^0.5.3",
    "eslint": "^5.4.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "react-highlight-updates": "^1.0.0",
    "wait-on": "^2.1.0"
  }
}
